#include <iostream>
#include <vector>
#include <cmath>
#include <cstdio>

std::vector<unsigned long> gr_code(int n)
{
	int len = pow(2, n);
	std::vector<unsigned long> vect(1);
	unsigned long power = 1, time_keeper = pow(2, power);
	for (unsigned i = 1; i < len; ++i)
	{
		if (i >= time_keeper)	time_keeper = pow(2, ++power);
		vect.push_back(vect[time_keeper - i - 1] + (1UL << (power - 1)));
	}
	return vect;
}

void print_as_2(const unsigned long num, unsigned long len)
{
	for (int i = len - 1; i >= 0; --i)
		std::cout << ((num >> i) & 1);
}

using namespace std;
int main()
{
	unsigned n;
	cin >> n;
	std::vector<unsigned long> vect = gr_code(n);
	for (const auto &el : vect)
	{
		print_as_2(el, n);
		cout << endl;
	}
	return 0;
}
